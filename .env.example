# UmiKarma Environment Variables
# Copy this file to .env.local and update with your actual values

# GitHub API (for future integration)
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id
REACT_APP_GITHUB_CLIENT_SECRET=your_github_client_secret

# OpenAI API (for AI summarization)
REACT_APP_OPENAI_API_KEY=your_openai_api_key

# Umi Network RPC (for smart contract interactions)
REACT_APP_UMI_RPC_URL=https://rpc.umi.network

# API Base URL (for backend integration)
REACT_APP_API_BASE_URL=http://localhost:3001

# WalletConnect Project ID (for wallet connectivity)
REACT_APP_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Feature Flags
REACT_APP_ENABLE_REAL_WALLET=false
REACT_APP_ENABLE_GITHUB_API=false
REACT_APP_ENABLE_AI_SUMMARY=false 